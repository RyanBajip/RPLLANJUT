<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bajip.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".f_mipa"
            android:label="Fakultas MIPA" />
        <activity
            android:name=".f_faperta"
            android:label="Fakultas Pertanian" />
        <activity
            android:name=".f_fpik"
            android:label="Fakultas Perikanan dan Kelautan" />
        <activity
            android:name=".f_fapet"
            android:label="Fakultas Peternakan" />
        <activity
            android:name=".f_fhut"
            android:label="Fakultas Kehutanan" />
        <activity
            android:name=".f_fem"
            android:label="Fakultas Ekonomi dan Manajemen" />
        <activity
            android:name=".f_fema"
            android:label="Fakultas Ekologi Manusia" />
        <activity
            android:name=".f_fkh"
            android:label="Fakultas Kedokteran Hewan" />
        <activity android:name=".FakultasActivity" />
        <activity android:name=".registerActivity"></activity>
    </application>

</manifest>